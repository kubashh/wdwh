{
  "name": "wdwh",
  "version": "1.7.8",
  "author": "kubashh",
  "description": "The Bun framework. Easyer web dev without html",
  "license": "MIT",
  "repository": "https://github.com/kubashh/wdwh",
  "keywords": [
    "react",
    "bun",
    "wdwh",
    "kubashh",
    "web",
    "html",
    "css",
    "typescript"
  ],
  "files": [
    "LICENSE",
    "README.md",
    "dist",
    "index.d.ts"
  ],
  "scripts": {
    "build": "bun build ./src/main.ts --outdir=dist --target=bun --minify --external=bun-plugin-tailwind",
    "pub": "bun scripts/publish.ts"
  },
  "bin": {
    "wdwh": "./dist/main.js"
  },
  "dependencies": {
    "bun-plugin-tailwind": "latest",
    "react": "latest",
    "react-dom": "latest",
    "tailwindcss": "latest"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@kubashh/nutil": "latest"
  },
  "prettier": {
    "semi": false,
    "printWidth": 108
  }
}
